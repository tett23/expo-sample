{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2017",
    "module": "es2015",
    "outDir": "lib",
    "sourceMap": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": ["dom", "es2015", "es2016"],
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitReturns": true,
    "declaration": true,
    "esModuleInterop": true
  }
}
